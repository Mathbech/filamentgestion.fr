{% extends 'base.html.twig' %}
{% block title %}Centre de contr√¥le{% endblock %}


{% block body%}

<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
                <h4 class="card-title">Ajout de bobines</h4>
                <p class="card-description"> Ajoutez vos bobines au stock </p>
            <div class="forms-sample">
                {{ form_start(form) }}
                {{ form_errors(form) }}
                <div class="form-group">
                    {{ form_row(form.type_imprimante) }}
                    </div>
                <div class="form-group">
                    {{ form_row(form.marque)}}
                </div>
                <div class="form-group">
                    {{ form_row(form.prix)}}
                </div>
                <div class="form-group">
                    {{ form_row(form.nom_imprimante)}}
                </div>
                <input type="submit" class="btn btn-primary mr-2" value="Envoyer" />
                {{ form_end(form) }}
                </div>
        </div>
    </div>
</div>

{% endblock %}